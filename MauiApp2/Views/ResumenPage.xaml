<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp2.Views.ResumenPage"
             Title="ResumenPage">
    <ScrollView>
        <VerticalStackLayout Padding="20" Spacing="15">
            <Label x:Name="NombreLabel" />
            <Label x:Name="IdentificacionLabel" />
            <Label x:Name="FechaLabel" />
            <Label x:Name="CorreoLabel" />
            <Label x:Name="SalarioLabel" />
            <Label x:Name="AporteIessLabel" FontAttributes="Bold" TextColor="Green" />

            <Button Text="Exportar a .txt" Clicked="Button_Clicked_1" />
        </VerticalStackLayout>
    </ScrollView>
</ContentPage>